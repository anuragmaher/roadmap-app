{
  "version": 2,
  "functions": {
    "backend/src/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "REACT_APP_API_URL": "/api"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
